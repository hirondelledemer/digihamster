{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:testing-library/react"
  ],
  "plugins": ["@typescript-eslint"],

  "rules": {
    "no-console": "warn",
    "@typescript-eslint/no-unused-vars": "error",
